<% include partials/header.ejs %>   

		<div id='map'></div>
		<h2><%=date %></h2>
		<div>
  			<% eventsList.forEach(function(event){ %>
        <a href="event/<%= event.venue.id %>/<%= event.id %>">
  				<h4><%=event.venue.name %></h4>
          <p><%=event.venue.city %>, <%=event.venue.region %></p>

  				<% event.artists.forEach(function(artist){ %>
  					<p><%=artist.name %></p>
  					<% }) %>
  				<% }) %>
      </a>
    </div>



